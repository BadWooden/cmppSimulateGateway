<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <!--cmpp20、30公用编码-->
    <bean id="cmppEncode" class="com.crazymouse.netty.handler.CmppEncode"/>
    <!--公用空闲检测handler-->
    <bean id="idleCheckHandler" class="com.crazymouse.netty.handlers.IdleStateCheckServerHandler"/>
    <!--cmpp20 start-->
    <bean id="cmpp20Decode" class="com.crazymouse.netty.handler.cmpp20.Cmpp20Decode"/>
    <bean id="cmpp20ServerHandler" class="com.crazymouse.netty.handler.cmpp20.Cmpp20ServerHandler">
        <property name="performance" ref="cmpp20Performance"/>
        <property name="resp_send" value="${cmpp20_respSend}"/>
        <property name="rpt_send" value="${cmpp20_rptSend}"/>
        <!--响应状态值：0为正常，-8 为流控错误-->
        <property name="reValue" value="${cmpp20_flowError}"/>
    </bean>
    <bean id="cmpp20Performance" class="com.crazymouse.netty.handler.util.Performance">
        <property name="name" value="cmpp20"/>
    </bean>
    <!--cmpp30 start-->
    <bean id="cmpp30Decode" class="com.crazymouse.netty.handler.cmpp30.Cmpp30Decode"/>
    <bean id="cmpp30ServerHandler" class="com.crazymouse.netty.handler.cmpp30.Cmpp30ServerHandler">
        <property name="performance" ref="cmpp30Performance"/>
        <property name="resp_send" value="${cmpp30_respSend}"/>
        <property name="rpt_send" value="${cmpp30_rptSend}"/>
        <!--响应状态值：0为正常，-8 为流控错误-->
        <property name="reValue" value="${cmpp30_flowError}"/>
    </bean>
    <bean id="cmpp30Performance" class="com.crazymouse.netty.handler.util.Performance">
        <property name="name" value="cmpp30"/>
    </bean>


</beans>
